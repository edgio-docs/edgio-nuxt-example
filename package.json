    "@edgio/prefetch": "^5.0.4",
    "@edgio/rum": "^5.0.8",
    "@edgio/vue": "^5.0.4",
    "@nuxt/postcss8": "^1.1.3",
    "@vercel/nft": "^0.20.1",
    "autoprefixer": "^10.4.7",
    "nuxt": "2.15.8",
